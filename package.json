{
  "name": "@radio4000/sdk",
  "version": "0.8.0",
  "description": "JavaScript client for Radio4000 for the browser and node",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/sdk.js",
  "exports": {
    ".": {
      "types": "./dist/types/main.d.ts",
      "import": "./dist/sdk.js"
    }
  },
  "types": "./dist/types/main.d.ts",
  "private": false,
  "scripts": {
    "dev": "vite --port 3999 --config vite-website.config.js",
    "build": "vite build && npm run types && npm run build-website",
    "build-website": "vite build --config vite-website.config.js",
    "preview": "vite preview",
    "check": "biome check --write && tsgo",
    "types": "tsgo",
    "test": "vitest run",
    "pull-db-types": "bunx supabase gen types typescript --project-id oupgudlkspsmzkmeovlh --schema public > src/database.types.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.96.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.2",
    "@types/node": "^25.2.3",
    "@typescript/native-preview": "^7.0.0-dev.20260217.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "author": "radio4000",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/radio4000/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/radio4000/sdk/issues"
  },
  "homepage": "https://github.com/radio4000/sdk#readme",
  "keywords": [
    "radio4000",
    "supabase",
    "sdk"
  ]
}
